<head>
    <% include ../Partials/AdminHeaderlink.ejs %>
</head>

<!--TIPS-->
<!--You may remove all ID or Class names which contain "demo-", they are only used for demonstration. -->
<body>
    <div id="container" class="effect aside-float aside-bright mainnav-lg">
        
        <!--NAVBAR-->
        <!--===================================================-->
        <% include ../Partials/Adminheader.ejs %>
        <!--===================================================-->
        <!--END NAVBAR-->

        <div class="boxed">

            <!--CONTENT CONTAINER-->
            <!--===================================================-->
            <div id="content-container">
                
                <!--Page Title-->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <div id="page-title">
                    <h1 class="page-header text-overflow">Modify Category</h1>

                    <!--Searchbox-->
                    <div class="searchbox">
                        <div class="input-group custom-search-form">
                            <input type="text" class="form-control" placeholder="Search..">
                            <span class="input-group-btn">
                                <button class="text-muted" type="button"><i class="demo-pli-magnifi-glass"></i></button>
                            </span>
                        </div>
                    </div>
                </div>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!--End page title-->

                  <!--Page content-->
                <!--===================================================-->
                <div id="page-content">
                    
					<div class="row">
					    <div class="col-lg-10 col-lg-offset-1">
					      
                                <div class="panel">
                                    <div class="panel-heading">
                                        <h3 class="panel-title">Edit Category</h3>
                                    </div>
                        
                                    <!--Horizontal Form-->
                                    <!--===================================================-->
                                    <form class="form-horizontal" method="post" action="/Admin/<%= Categoredit._id %>/update">
                                        <div class="panel-body">
                                                <p >
                                                        <% if( typeof errors!="undefined"){ %>
                                                            <ul>
                                                            <% for(var i = 0; i < errors.length; i++){ %>
                                                                <li style="color:red!important"> <%= errors[i].msg %> </li>
                                                            <% } %>
                                                            </ul>
                                                         <% } %>
                                                    </p>
                                                    <input type="hidden"  name="datecreated" class="form-control" value="<%= Categoredit.DateCreated %>">
                                                    <input type="hidden"  name="isactive" class="form-control"  value="<%= Categoredit.Isactive %>">
                                            <div class="form-group">
                                                <label class="col-sm-3 control-label" for="demo-hor-inputemail">Category Name</label> 
                                                <div class="col-sm-9">
                                                    <input type="Text"  name="Categoryname" class="form-control" value="<%= Categoredit.Name %>">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-sm-3 control-label" for="demo-hor-inputemail">Description</label>
                                                <div class="col-sm-9">
                                                    <input type="Text"  name="Catdescript" class="form-control" value="<%= Categoredit.Description %>">
                                                </div>
                                            </div>
                                     
                                        </div>
                                        <div class="panel-footer text-right">
                                            <button class="btn btn-success" type="submit">Update</button>
                                        </div>
                                    </form>
                                    <!--===================================================-->
                                    <!--End Horizontal Form-->
                        
                                </div>
                            
					    </div>
					</div>
					
					
					
                </div>
                <!--===================================================-->
                <!--End page content-->


            </div>
            <!--===================================================-->
            <!--END CONTENT CONTAINER-->
            
            <!--MAIN NAVIGATION-->
            <!--===================================================-->
            <% include ../Partials/Sidebar.ejs %>
            <!--===================================================-->
            <!--END MAIN NAVIGATION-->

        </div>

        

        <!-- FOOTER -->
        <!--===================================================-->
        <footer id="footer">

            <!-- Visible when footer positions are fixed -->
            <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
            <div class="show-fixed pull-right">
                You have <a href="#" class="text-bold text-main"><span class="label label-danger">3</span> pending action.</a>
            </div>



            <!-- Visible when footer positions are static -->
            <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
            <div class="hide-fixed pull-right pad-rgt">
                14GB of <strong>512GB</strong> Free.
            </div>



            <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
            <!-- Remove the class "show-fixed" and "hide-fixed" to make the content always appears. -->
            <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->

            <p class="pad-lft">&#0169; 2017 Your Company</p>



        </footer>
        <!--===================================================-->
        <!-- END FOOTER -->


        <!-- SCROLL PAGE BUTTON -->
        <!--===================================================-->
        <button class="scroll-top btn">
            <i class="pci-chevron chevron-up"></i>
        </button>
        <!--===================================================-->



    </div>
    <!--===================================================-->
    <!-- END OF CONTAINER -->




</body>

